  "license": "ISC",
  "dependencies": {
    "discord.js": "^12.5.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@ffflorian/jszip-cli": "^3.1.0",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.37",
    "copyfiles": "^2.4.1",
    "rimraf": "^3.0.2"
  }
}
